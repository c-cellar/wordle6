{
  "name": "wordle6",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "npm-run-all --parallel sass-dev postcss-dev server",
    "build": "npm-run-all sass-build postcss-build",
    "server": "browser-sync start --server --watch",
    "sass-dev": "sass sass:temp --watch",
    "sass-build": "sass sass:temp",
    "postcss-dev": "postcss temp/*.css --dir assets/css --use postcss-preset-env --map --watch",
    "postcss-build": "postcss temp/*.css --dir assets/css --use postcss-preset-env cssnano --map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c-cellar/wordle6.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/c-cellar/wordle6/issues"
  },
  "homepage": "https://github.com/c-cellar/wordle6#readme",
  "devDependencies": {
    "browser-sync": "^2.27.10",
    "cssnano": "^5.1.12",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "postcss-preset-env": "^7.7.2",
    "sass": "^1.53.0"
  },
  "browserlist": [
    "defaults",
    "not IE 11"
  ]
}
